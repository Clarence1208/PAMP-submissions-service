  "group_uuid": "987fcdeb-51a2-43d1-9f12-345678901234",
  "project_step_uuid": "222e2222-2222-2222-2222-222222222222",
  "description": "GitHub repository submission",
  "submitted_by_uuid": "bbb2b222-2222-2222-2222-222222222222"
